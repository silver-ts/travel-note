{
  "name": "travel-note-server",
  "version": "0.1.0",
  "private": true,
  "description": "Express server for travel note",
  "keywords": [
    "express.js",
    "jest",
    "mongoose",
    "travel-note"
  ],
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --watchAll --coverage --verbose --silent --runInBand --env node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@ryuuto829/eslint-config-with-prettier": "^0.3.0",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.16",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "rate-limit-mongo": "^2.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mongodb-memory-server": "^6.9.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  }
}
